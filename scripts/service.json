{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "labels":{"component":"spring-boot-docker","container":"spring-boot-docker","hystrix.enabled":"true","hystrix.cluster":"default"},
	"name": "spring-boot-service"
    },
    
    "spec": {
        "selector": {
            "app": "spring-boot-docker"
        },
        "ports": [
            {
                "protocol": "TCP",
                "port": 80,
                "targetPort": 8080 
            }
        ]
    }
}
